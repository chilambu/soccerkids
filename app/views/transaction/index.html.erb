<h1>Listing transactions</h1>

<table >
  <tr>
    <th>First name</th>
    <th>Middle name</th>
    <th>Last name</th>
    <th>Gender</th>
    <th>Dob</th>
    <th>User</th>
    <th>School name</th>
    <th>Registrant info</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @transaction.each do |transaction| %>
  <tr>
    <td><%= transaction.first_name %></td>
    <td><%= transaction.middle_name %></td>
    <td><%= transaction.last_name %></td>
    <td><%= transaction.gender %></td>
    <td><%= transaction.dob %></td>
    <td><%= transaction.user_id %></td>
    <td><%= transaction.school_name %></td>
    <td><%= transaction.registrant_info %></td>
    <td><%= link_to 'Show', transaction %></td>
    <td><%= link_to 'Edit', edit_transaction_path(transaction) %></td>
    <td><%= link_to 'Destroy', transaction, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New transaction', new_transaction_path %>
