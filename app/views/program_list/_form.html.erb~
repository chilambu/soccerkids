<%= form_for @program_list, :url =>"create" do |f| %>
  <% if @program_list.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@program_list.errors.count, "error") %> prohibited this program from being saved:
      </h2>
      <ul>
      <% @program_list.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <div class="field">
   Name
    <%= f.text_field :name %>
    <%= f.hidden_field :program_id,  :value => params[:pg]%>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
